body,caption
{
	font-family: Verdana, Arial, Helvetica;
	font-size: 7pt;
}

div.zGroup,caption.zGroup,div.zhelpgroup
{
	font-size: 7pt;
	font-weight: bold;
	text-align: center;
	text-transform: uppercase;
	background-color : #ffffff; /*Gray;*/
	color:White ;
	
}

span.zCaption
{
	font-size: 7pt;
	font-weight: bold;
	text-align: center;
	text-transform: uppercase;
}
pre
{
	font-family: Verdana, Arial, Helvetica;
	font-size: 7pt;
	white-space: pre-wrap; /* CSS3 - Text module (Candidate Recommendation) http://www.w3.org/TR/css3-text/#white-space */
	word-wrap: break-word; /* IE 5.5+ */
}

table
{
border-collapse: collapse;
empty-cells: show;
}

td
{
	font-family: Verdana, Arial, Helvetica;
	font-size: 7pt;
	vertical-align: top;
}

th
{
	font-family: Verdana, Arial, Helvetica;
	font-size: 7pt;
	text-align: center;
}
th.zfooter
{
	text-align:left;
	color:black;
	font-weight:normal;
}

span.zName
{
	font-size: 14pt;
	font-weight: bold;
}
col.zProperty
{
	
	font-weight: bold;
	text-align: right;
	width: 104px;	
	white-space:nowrap;
}
td.zProperty
{
	
	font-weight: bold;
	text-align: right;
	width: 104px;	
	white-space:nowrap;
}
tr.zColumnHeader{
	
	background-color: Gainsboro;  
	font-weight: bold;
	vertical-align: top;
	text-align: center;
}
tr.mainheader{
	color: #ffffff;  
	background-color: #204080;  
}
/*------------HTML LIST-----------*/
div.htmllist_topdiv{
	   width:100%;
	   height:50px;
	   overflow:hidden;
}
div.htmllist_maindiv{
	   width:100%;
	   overflow-x:scroll;
	   border:1px solid black;
}
img.htmllist_icon{
	border:0px;
	height:32px;
	width:32px;
}
thead.htmllist_columnheader{
	cursor: pointer;
}
